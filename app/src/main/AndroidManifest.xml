<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrclaim">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mrclaim"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mrclaim"
        android:supportsRtl="true"
        android:theme="@style/Theme.MrClaim">
        <activity android:name=".ResetPasswordActivity"></activity>
        <activity android:name=".MyAccident" />
        <activity android:name=".ReportAccident" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".register" />
    </application>

</manifest>